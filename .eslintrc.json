{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
      "radix":"off",
      "arrow-parens": "off",
      "no-param-reassign": "off"
    }
}